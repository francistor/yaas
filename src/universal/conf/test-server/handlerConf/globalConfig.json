{
	"sessionCDRDirectories": "/var/yaas/cdr/session-/var/yaas/cdr/session-hot",
	"serviceCDRDirectories": "/var/yaas/cdr/service",
	"writeSessionCDR": true,
	"writeServiceCDR": true,
	"cdrFilenamePattern": "cdr_%d{yyyyMMdd}.txt",
	"provisionType": "database",
	"authLocal": "provision",
	"sendReject": "yes",
	"rejectServiceName": "reject",
	"blockingServiceName": "pcautiv",
	"rejectFilter": "portal",
	"proxyTimeoutMillis": 500,
	"proxyRetries": 1,
	"acceptOnProxyError": false,
	"inlineProxyGroupName": "yaas-superserver-group",
	"authProxyFilterOut": "standardProxyFilterOut.json",
	"authProxyFilterIn": "standardProxyFilterIn.json",
	"acctProxyFilterOut": "standardProxyFilterOut.json",
	"radiusAttrs":[
		{"Session-Timeout": 3600}
	],
	"nonOverridableRadiusAttrs": []
}
